<resources>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="raised" parent="Theme.AppCompat.Light">
        <item name="colorButtonNormal">#2E8BA0</item>
        <item name="colorControlHighlight">#FFFF</item>
    </style>

    <style name="SpinnerTheme">
        <item name="android:textColor">#333333</item>
        <item name="android:fontFamily">@font/bs</item>
        <item name="android:textSize">@dimen/texto</item>
    </style>

    <style name="Splash" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
</resources>